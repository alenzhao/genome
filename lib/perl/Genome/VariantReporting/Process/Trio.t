#!/usr/bin/env genome-perl

BEGIN { 
    $ENV{UR_DBI_NO_COMMIT} = 1;
    $ENV{UR_USE_DUMMY_AUTOGENERATED_IDS} = 1;
}

use strict;
use warnings;

use above "Genome";
use Test::More;

my $pkg = 'Genome::VariantReporting::Process::Trio';

use_ok($pkg);
my $base = __FILE__.".d";
my $file1 = File::Spec->join($base, "file1");
my $file2 = File::Spec->join($base, "file2");
my $file3 = File::Spec->join($base, "file3");

is(0, Genome::VariantReporting::Process::Trio::compare_alignment_summary($file1, $file2));
is(1, Genome::VariantReporting::Process::Trio::compare_alignment_summary($file1, $file3));

done_testing;

